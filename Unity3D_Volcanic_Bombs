using UnityEngine;
using System.Collections;

public class VolcanicBomb : MonoBehaviour {

	// Use this for initialization
	void Start () {

	}
	
	// Update is called once per frame
	void Update () {

	}

	void OnTriggerEnter2D (Collider2D trig) {
		Destroy(gameObject);
	}

	void OnCollisionEnter2D (Collision2D coll) {
		Destroy(gameObject);

	}

}
